{
  "name": "jwt-base-nodejs",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/express": "^4.17.15",
    "@types/node": "^16.11.10",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/helmet": "^4.0.0",
    "@types/swagger-jsdoc": "^6.0.1"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "big.js": "^6.2.1",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "express-rate-limit": "^6.6.0",
    "helmet": "^5.1.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.19",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.5.0",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "0.3.7",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -r -f dist && tsc --project tsconfig.json",
    "build:staging": "cross-env NODE_ENV=staging rm -r -f dist && tsc --project tsconfig.json",
    "dev": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=production node -r ./tsconfig-paths.js dist/index.js",
    "typeorm": "cross-env NODE_ENV=production && npx typeorm-ts-node-commonjs -t ./src/data-source.ts",
    "typeormnew": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "migration:gen": "npm run typeormnew migration:generate",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run",
    "typeorm-product": "cross-env NODE_ENV=production node -r ./tsconfig-paths.js ./node_modules/typeorm/cli.js -d ./dist/data-source.js",
    "migration-product:gen": "npm run typeorm-product migration:generate"
  }
}